{
	"name": "generate-app",
	"version": "1.0.0",
	"description": "",
	"main": "app.js",
	"scripts": {
		"start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --exit-child src/app.ts",
		"start:prod": "node dist/src/app.js",
		"build": "tsc -p .",
		"typeorm": "typeorm-ts-node-commonjs",
		"docker-compose": "docker compose up -d",
		"docker-compose:dev": "docker compose -f docker-compose.yml -f docker-compose.development.yml up -d",
		"docker-compose:prod": "docker compose -f docker-compose.yml -f docker-compose.production.yml up -d",
		"db:init": "pnpm migrate && pnpm db:push",
		"migrate": "rm -rf dist && pnpm build && pnpm typeorm migration:generate ./src/migrations/addedEntity -d ./src/utils/dataSource.ts",
		"db:push": "rm -rf dist && pnpm build && pnpm rebuild bcrypt && pnpm typeorm migration:run -d src/utils/dataSource.ts",
		"db:reset": "pnpm db:drop && pnpm db:init",
		"db:drop": "rm -rf dist && pnpm build && pnpm typeorm migration:revert -d src/utils/dataSource.ts",
		"db:migrate:prod": "pnpm typeorm migration:run -d dist/src/utils/dataSource.js",
		"prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
		"type-check": "tsc --noEmit",
		"lint": "eslint src/**/*.ts --fix",
		"lint-staged": "lint-staged",
		"prepare": "husky",
		"audit-fix": "pnpm up && pnpm audit fix",
		"security-audit": "bash scripts/security-audit.sh",
		"prestart:prod": "pnpm security-audit",
		"dependency-graph": "node scripts/generate-dependency-graph.js",
		"version:patch": "npm version patch",
		"version:minor": "npm version minor",
		"version:major": "npm version major",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
